{
  "rules": {
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "import/no-extraneous-dependencies": "off",
    "no-return-await": "off",
    "no-plusplus": "off",
    "no-console": "off",
    "no-shadow": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off"
  },

  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ]
}